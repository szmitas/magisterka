<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.czytnikrss"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="czytnikrss.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="czytnikrss.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="czytnikrss.LatestNewsActivity"
            android:label="@string/title_activity_latest_news" >
        </activity>
        <activity
            android:name="czytnikrss.NewsListActivity"
            android:label="@string/title_activity_news_list" >
        </activity>
        <activity
            android:name="czytnikrss.NewsActivity"
            android:label="@string/title_activity_news" >
        </activity>
    </application>

</manifest>